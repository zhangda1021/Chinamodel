 266  );
 267  );
 268  );
 269  *vom(i,r) = sum(j_,vom_(i,j_,r));
 270  *display vom;
 271   
 272  *       Factor payments by sector:
 273  vfm(f,i,r) = sum(mapf(j_,f),sum(mapic(i_,i),sam(r,j_,i_)));
 274  *display vfm;
 275   
 276  *       Intermediate demands for Armington goods:
 277  vafm(j,i,r) =  sum((mapic(i_,i),mapjc(j_,j)),sam(r,j_,i_));
 278  vafm(j,"c",r) =  sum((maph(i_,g),mapjc(j_,j)),sam(r,j_,i_));
 279  vafm(j,"i",r) =  sum((mapinv(i_,g),mapjc(j_,j)),sam(r,j_,i_));
 280  vafm(j,gov,r) =  sum((mapg(i_,gov),mapjc(j_,j)),sam(r,j_,i_));
 281   
 282  *       Domestic and foreign exports and immports:
 283  vxm(i,r,"dtrd")  = sum(mapjc(i_,i),sam(r,i_,"70"));
 284  vxm(i,r,"ftrd")  = sum(mapjc(i_,i),sam(r,i_,"71"));
