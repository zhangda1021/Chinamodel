 337  parameter
 338          vam     Armginton production;
 339   
 340  vam(i,r) = sum(trd,vim(i,r,trd)) + vom(i,r);
 341   
 342   
 343  *       Check sparsity:
 344   
 345  parameter total_supply,intermediate_demand;
 346  total_supply(i,r) = vom(i,r) + sum(trd,vim(i,r,trd)) + vinvtd(i,r);
 347  intermediate_demand(j,i,r) = vafm(j,i,r);
 348   
 349  parameter chk_vafm;
 350  *chk_vafm(i,g,r) = yes$(vafm(i,g,r) and (not sum(trd,vim(i,r,trd))));
 351  chk_vafm(i,j,r) = yes$(intermediate_demand(i,j,r) and (not total_supply(i,r)));
 352   
 353  display chk_vafm;
